<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yht.dao.LoginDao">
    <!--用户登录-->
    <select id="login" resultType="map">
          select userid,username,password from t_account where username = #{username} and password = #{password}
    </select>
</mapper>